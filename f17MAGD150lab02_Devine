void setup()
{
  size(600,600);
  background(0);
}
void draw()
{

  fill(60,80,200);
  arc(250,300,180,180,0,180);
  fill(60,200,80);
  arc(250,300,120,120,0,180);
  
  fill(240,230,60);
  arc(80,100,150,150,0,180);
  
  fill(250,80,80);
  arc(300,100,80,80,0,180);

  fill(200,170,50);
  arc(400,100,90,90,0,180);
  
  fill(255,0,0);
  arc(500,200,70,70,0,180);
  
fill(160,200,80);
beginShape();
 vertex(350,230);
 vertex(300,250);
 vertex(350,270);
 endShape(CLOSE);
}
